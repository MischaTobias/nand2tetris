/**
 * 16-way Or: 
 * out = (in[0] or in[1] or ... or in[15])
 */

CHIP Or16Way {
    IN in[16];
    OUT out;

    PARTS:
    Or8Way(in=in[0..7], out=v1);
    Or8Way(in=in[8..15], out=v2);
    Or(a=v1, b=v2, out=out);
}
